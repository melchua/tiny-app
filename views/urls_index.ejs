<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/_bootstrap %>

</head>
  <% include partials/_body %>
  <% include partials/_header %>

  <ul class="list-group">
    <% for (url in urlsF) { %>
    <li class="list-group-item">

      <a href="/u/<%= url %>"><%= url %></a> &#8594;

      <a href="<%= urlsF[url].longURL %>"><%= urlsF[url].longURL %></a>

      <form method="GET" action="/urls/<%= url %>">
        <button type="submit" class="btn btn-default">EDIT</button>
      </form>

      <form method="POST" action="/urls/<%= url %>/delete">
        <button type="submit" class="btn btn-default">DELETE</button>
      </form>
    </li>
    <% } %>
  </ul>

  <a href="/urls/new"> Create new shortened link </a>
</body>
</html>


